{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "strict": false,
        "jsx": "preserve",
        "sourceMap": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": [
            "esnext",
            "dom"
        ]
    },
    "include": [
        "src/**/*.js",
        "src/**/*.vue",
        "src/**/*.json"
    ],
    "exclude": [
        "node_modules"
    ],
    "vueCompilerOptions": {
        "target": 3.3
    }
}